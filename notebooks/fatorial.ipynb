   "source": [
    "# Mpi Fatorial\n",
    "\n",
    "function fat(n)\n",
    "    if n == 0\n",
    "       return 1\n",
    "    else\n",
    "        return n*fat(n-1)\n",
    "    end\n",
    "end\n",
    "\n",
    "function main()\n",
    "    n = 10\n",
    "    f = fat(n)\n",
    "    print(\"Para o vlr $n temos o fat = $f\")\n",
    "end\n",
    "\n",
    "main()"
   ]
